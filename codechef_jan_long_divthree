Problem: https://www.codechef.com/JAN21C/problems/DIVTHREE
************************************************************
Solution:

#include <iostream>
#include<vector>
using namespace std;

int main() {
	// your code goes here
	long long int t,n,k,d,sum;
	vector<int> arr(100);
	cin>>t;
	while(t)
	{
	    sum=0;
	    cin>>n>>k>>d;
	    for(int i=0;i<n;i++)
	    {
	        cin>>arr[i];
	        sum+=arr[i];
	    }
	    if((sum/k)>=d)
	    cout<<d<<endl;
	    else
	    cout<<sum/k<<endl;
	        
	    
	    
	    
	    t--;
	   
	}
	
	return 0;
}
